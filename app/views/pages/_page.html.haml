.container
  %h4.center= @page.title
  .row
    - if @page.show_calendar?
      = render "calendar"
    - unless @page.show_calendar?
      - unless @page.content.blank?
        .col.s12
          .card-panel
            = @page.content.html_safe
      - unless @page.collapsibles.empty?
        .col.s12
          = render @page.collapsibles.page_filtered
